@page "/Classifiers/InstitutionType/Index"

@inject IClassifierRepository<InstitutionType> classifierRepository

<ClassifierTable Classifiers="@classifiers" ClassifierName="InstitutionType"></ClassifierTable>

@code {
    List<InstitutionType> classifiers;

    protected override async Task OnInitializedAsync()
    {
        classifiers = await Task.Run(() => classifierRepository.GetAll());
    }

}
